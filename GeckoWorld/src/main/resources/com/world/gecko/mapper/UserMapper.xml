<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.world.gecko.mapper.UserMapper">
	<select id="getUserById"
		resultType="com.world.gecko.domain.UserVo" parameterType="map">
		SELECT * from
		user_ where user_id = #{user_id}
	</select>
	<insert id="addUser" parameterType="com.world.gecko.domain.UserVo">
		INSERT into user_ (user_id, user_nickname, user_pw, user_role, login_type)
		values(#{user_id},#{user_nickname},#{user_pw}, #{user_role}, #{login_type})
	</insert>
</mapper>